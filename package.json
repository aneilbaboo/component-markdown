{
  "name": "markdown-components",
  "version": "1.0.1",
  "description": "Add user-safe, React-like components to your favorite markdown engine",
  "main": "lib/markdown-components.js",
  "browser": "lib/markdown-components.min.js",
  "repository": "https://github.com/aneilbaboo/markdown-components",
  "author": "Aneil Mallavarapu",
  "license": "MIT",
  "scripts": {
    "build": "WEBPACK_ENV=build webpack",
    "report": "nyc report",
    "debugTest": "mocha --inspect-brk --require babel-core/register --colors ./test/*.spec.js",
    "dev": "WEBPACK_ENV=dev webpack --progress --colors --watch",
    "prepublish": "npm run build",
    "lint": "eslint ./src/**/*.js",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "watch": "mocha --require babel-core/register --colors -w ./test/**/*.spec.js"
  },
  "dependencies": {
    "memory-streams": "^0.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "lodash": "^4.17.4",
    "markdown": "^0.5.0",
    "markdown-it": "^8.4.0",
    "markdown-js": "^0.0.3",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "showdown": "^1.8.6",
    "uglify-js": "https://github.com/mishoo/UglifyJS2.git#harmony-v2.8.22",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "clover",
      "html",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
